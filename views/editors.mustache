{{>header}}

{{>navigation}}

<div class="row">
  <div class="leftcolumn">
    <div class="card">
      <h2>Editors</h2>
      <p>Only editors should be able to view this page!</p>
      <h3>Users</h3>
      <table class="table table-bordered">
        <thead class="table-dark">
        <tr>
          <th>UserName</th>
          <th>Password</th>
          <th>Level</th>
          <th>Delete</th>
          </tr>
          </thead>
        <tbody>
        {{#Userdata}}
        <tr>
          <td>{{username}}</td>
          <td>{{password}}</td>
          <td>{{level}}</td>
          <td><a href="/editors/delet/{{username}}">D</a></td>
        </tr>
        {{/Userdata}}
        </tbody>
      </table>

      <h3>Articels</h3>
      <table class="table table-bordered">
        <thead class="table-dark">
        <tr>
          <th>Title</th>
          <th>Author</th>
          <th>Delete</th>
          </tr>
          </thead>
        <tbody>
        {{#Articledata}}
        <tr>
          <td>{{title}}</td>
          <td>{{username}}</td>
          <td><a href="/editors/delete/{{title}}">D</a></td>
        </tr>
        {{/Articledata}}
        </tbody>
      </table>

    </div>
  </div>

  {{>sidebar}}

</div>

{{>footer}}
